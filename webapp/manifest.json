{
	"_version": "1.5.0",
	"sap.app": {
		"id": "com.arcelor.scm.ordertrack",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "com.arcelor.scm.ordertrack.view.S0_App",
			"type": "XML",
			"async": true,
			"Ã¯d": "appStart"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.arcelor.scm.ordertrack.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}, 
		"routing": {
			"config": 
				{
				"routerClass": "sap.m.routing.Router",
				"viewPath": "com.arcelor.scm.ordertrack.view",
				"viewType": "XML",
				"controlId": "appStart",
				"controlAggregation": "pages",
				"clearAggregation": false,
				"transition": "slide",
				"async": true,
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"name": "mainScreen",
					"pattern": "",
					"target": [
						"s1MainScreen"
					]
				}
			],
			"targets": {
				"s1MainScreen": {
					"viewId": "s1MainScreen",
					"viewType": "XML",
					"viewName": "S1_MainScreen",
					"viewLevel": 1
				}, 
				"notFound": {
					"viewId": "notFound",
					"viewName": "NotFound",
					"viewType": "XML",
					"transition": "show"
				}
			}
		}
	}
}